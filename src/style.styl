body {
  padding: 0;
  margin: 0;
}

.el-header {
  background-color: #1abc9c;
  color: #fff;
  line-height: 60px;
  position: fixed;
  width: 100%;
  z-index: 1992;
}

.toolbar {
  margin-bottom: 15px;
}

.aside-bar {
  position: fixed;
  min-height: calc(100vh - 60px);
  background-color: #f5f5f5;
}

.container-inner {
  width: 88%;
}